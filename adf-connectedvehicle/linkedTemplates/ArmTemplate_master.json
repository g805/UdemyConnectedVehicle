{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-connectedvehicle"
		},
		"ls_adlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsgen2'"
		},
		"ls_adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saconnectedvehicleud.dfs.core.windows.net/"
		},
		"ls_akv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KV-connectedvehicle.vault.azure.net/"
		},
		"ls_amzns3_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ls_akv",
					"type": "LinkedServiceReference"
				},
				"secretName": "accesskeyaws"
			}
		},
		"ls_azsqldb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "connectedvehicleserver.database.windows.net"
		},
		"ls_azsqldb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "connectedvehicledatabase"
		},
		"ls_azsqldb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "gkadmin"
		},
		"trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/a680d083-036d-4aad-b087-4918c50144f4/resourceGroups/rg_connected_vehicle_ud/providers/Microsoft.Storage/storageAccounts/saconnectedvehicleud"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-connectedvehicle_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adlsgen2_accountKey": {
						"value": "[parameters('ls_adlsgen2_accountKey')]"
					},
					"ls_adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen2_properties_typeProperties_url')]"
					},
					"ls_akv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_akv_properties_typeProperties_baseUrl')]"
					},
					"ls_amzns3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('ls_amzns3_properties_typeProperties_accessKeyId')]"
					},
					"ls_azsqldb_properties_typeProperties_server": {
						"value": "[parameters('ls_azsqldb_properties_typeProperties_server')]"
					},
					"ls_azsqldb_properties_typeProperties_database": {
						"value": "[parameters('ls_azsqldb_properties_typeProperties_database')]"
					},
					"ls_azsqldb_properties_typeProperties_userName": {
						"value": "[parameters('ls_azsqldb_properties_typeProperties_userName')]"
					},
					"trigger_properties_typeProperties_scope": {
						"value": "[parameters('trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}